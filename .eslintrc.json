{
  "env": {
    "browser": true,
    "jquery": true
  },
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "rules": {
    "no-plusplus": 0,
    "max-len": ["error", { "code": 120, "ignoreUrls": true }],
    "no-param-reassign": ["error", { "props": false }],
    "no-use-before-define": "off"
  }
}
